<form name="FormNewList">
  <div class="form-group">
    <label for="InputTitle">Titulo da lista *</label>
    <input type="text" class="form-control" id="InputTitle" placeholder="Titulo" ng-model="list.title" required>
  </div>

  <div class="form-group">
    <label for="TextDescription">Descrição</label>
    <textarea rows="8" cols="80" class="form-control" id="TextDescription" placeholder="Descrição" ng-model="list.description"></textarea>
  </div>
</form>
